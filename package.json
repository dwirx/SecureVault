{
	"name": "securevault-plus",
	"version": "1.0.0",
	"description": "AES-256 folder encryption for Obsidian - secure, cross-platform, easy to use",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": ["obsidian", "encryption", "security", "privacy", "vault"],
	"author": "SecureVault Team",
	"license": "MIT",
	"dependencies": {
		"crypto-js": "^4.2.0"
	},
	"devDependencies": {
		"@types/crypto-js": "^4.2.1",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
